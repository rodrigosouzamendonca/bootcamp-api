    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-validator": "^5.2.0",
    "jwt-simple": "^0.5.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "sequelize": "^4.37.7",
    "sqlite3": "^4.0.0"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "nodemon": "^1.17.4"
  }
